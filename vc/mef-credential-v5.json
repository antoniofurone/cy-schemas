{
  "$id": "https://github.com/antoniofurone/cy-schemas/raw/master/vc/mef-credential-v5.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "MEFMembershipCredential",
  "description": "MEF Membership Credential",
  "type": "object",
  "properties": {
   "credentialSubject": {
     "type": "object",
     "additionalProperties": false,
     "properties": {
         "credential_type": {
           "type": "string",
           "description": "type of credential",
           "pattern": "^[\\s\\S]*$"
         }, 
         "start_date": {
           "type": "string",
           "description": "credential start date",
           "pattern": "^[\\s\\S]*$"
           }          
       }          
   }
 }
}