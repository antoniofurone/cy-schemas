{
   "$id": "https://github.com/antoniofurone/cy-schemas/raw/master/vc/mef-credential-v1.json",
   "$schema": "https://json-schema.org/draft/2020-12/schema",
   "title": "MEFMembershipCredential",
   "description": "MEF Membership Credential",
   "type": "object",
   "properties": {
    "credentialSubject": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "pattern": "^[A-Za-z0-9]{3,10}$"
        },
        "membership":{
            "type":"object",
            "properties":{
                "type": {
                  "type": "string",
                  "pattern": "^[A-Za-z0-9]{3,10}$"
                }, 
                "start_date": {
                  "type": "string",
                  "pattern": "^[A-Za-z0-9]{3,10}$"

                  }          
                }          
            }
        }
      }
    }
  }
}        